      <Link>Tools\NuGet.exe</Link>
    </NuGetPackage>
  </ItemGroup>
  <!-- Target to build Squirrel release -->
  <Target Name="AfterBuild" Condition=" '$(Configuration)' == 'Release'">
    <GetAssemblyIdentity AssemblyFiles="$(TargetPath)">
      <Output TaskParameter="Assemblies" ItemName="myAssemblyInfo" />
    </GetAssemblyIdentity>
    <!-- Get our version number -->
    <GetVersionParts AssemblyPath="$(TargetPath)">
      <Output TaskParameter="MajorVersion" PropertyName="MajorVersionNumber" />
      <Output TaskParameter="MinorVersion" PropertyName="MinorVersionNumber" />
      <Output TaskParameter="BuildVersion" PropertyName="BuildVersionNumber" />
      <Output TaskParameter="FriendlyVersion" PropertyName="ReleaseFriendlyVersion" />
    </GetVersionParts>
    <Exec Command="@(nuGetPackage->'%(fullpath)') pack ..\Publish\CIC.nuspec -Properties Configuration=Release;Version=$(ReleaseFriendlyVersion) -OutputDirectory $(OutDir) -BasePath $(OutDir)" />
    <Exec Command="@(squirrelPackage->'%(fullpath)') --releasify $(OutDir)CIC.$(ReleaseFriendlyVersion).nupkg" />
  </Target>
